<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="libraries"
            type="com.yoesuv.infomalangbatu.menu.other.viewmodels.ChildFragmentLibrariesViewModel"/>
    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewListLibraries"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:paddingTop="@dimen/fragment_other_global_margin"
        android:paddingBottom="@dimen/fragment_other_global_margin"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        tools:listitem="@layout/item_libraries"/>

</layout>